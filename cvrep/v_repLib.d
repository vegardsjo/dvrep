extern (C) void *dlopen(const char*, int);
extern (C) int dlclose(void*);
extern (C) void* dlsym(void*, const char*);
extern (C) char* dlerror();

void* loadVrepLibrary(const char* pathAndFilename)
{
  void *lib = dlopen(pathAndFilename, 2);

  char *error = dlerror();
  if (error != null)
    throw new Exception(to!string(error));

  return lib;
}

void unloadVrepLibrary(void* lib) {
  dlclose(lib);
}

void procBind(T)(void *lib, ref T proc, const char* funcName) {
  proc = cast(T)(dlsym(lib, funcName));

  if (proc == null)
    throw new Exception("Could not bind function %s".format(funcName));
}

